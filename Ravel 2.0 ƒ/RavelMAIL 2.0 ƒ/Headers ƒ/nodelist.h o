void ReadNodeList (void);void CloseNodeList (void);Boolean GetNodeInfo (addr *, StringPtr);Boolean GetNameFromAddr (addr *, StringPtr);Boolean GetPhoneFromAddr (addr *, StringPtr);Boolean GetTimesFromAddr (addr *, _period *);#define	f_zone		0x0001	//	zone coordinator#define	f_region	0x0002	//	region coordinator#define	f_host		0x0004	//	net host#define	f_hub		0x0008	//	net hub#define	f_down		0x0010	//	down node#define	f_hold		0x0020	//	hold node#define	f_pvt		0x0040	//	private node#define	f_point		0x8000	//	it's point